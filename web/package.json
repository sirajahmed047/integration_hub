{
  "name": "carbon-tutorial-nextjs",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:carbon-design-system/carbon-tutorial-nextjs.git",
  "bugs": "https://github.com/carbon-design-system/carbon-tutorial-nextjs/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\""
  },
  "dependencies": {
    "@carbon/icons-react": "^11.39.0",
    "@carbon/pictograms-react": "^11.50.0",
    "@carbon/react": "^1.42.0",
    "@carbon/styles": "^1.42.0",
    "@octokit/core": "4.2.0",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "carbon-components": "^10.58.12",
    "carbon-components-react": "^8.54.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "i": "^0.3.7",
    "next": "13.5.6",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18",
    "sass": "^1.69.0",
    "sharp": "^0.33.4",
    "swc": "^1.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "7.5.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.25",
    "husky": "1.3.1",
    "lint-staged": "^15.2.5",
    "prettier": "^2.8.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --cache --write",
      "git add"
    ],
    "*.{scss,css}": [
      "prettier --cache --write",
      "git add"
    ],
    "*.md": [
      "prettier --cache --write",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
